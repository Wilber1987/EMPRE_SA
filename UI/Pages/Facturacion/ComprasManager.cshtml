@page
@using API.Controllers
@using APPCORE.Security
@{
     if (!SecurityController.Auth(HttpContext.Session.GetString("seassonKey")))
    {
        Response.Redirect("../Security/login");
        return;
    }
    @if (!SecurityController.HavePermission(Permissions.GESTION_COMPRAS.ToString(), HttpContext.Session.GetString("seassonKey")) )//todo
    {
       Response.Redirect("../Security/login");
        return;
    }    
}
<script src='~/Facturacion/Views/ComprasManagerView.js' type='module'></script>
<div id='MainBody'></div>
